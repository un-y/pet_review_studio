<%= render 'admin/shared/list', items: @deshboards %>

<div class="col-md-9">
  <h3>全ユーザー一覧</h3>
  <ul>
    <% @users.each do |user| %>
      <li>
        <%= user.id %> - <%= user.name %> - <%= user.email %>
        <%= link_to '削除', admin_user_path(user), method: :delete, data: { confirm: '本当に削除しますか？' } %> 
      </li>
    <% end %>
  </ul>

  <h3>全ペットジャンル一覧</h3>
  <ul>
    <% @pet_genres.each do |pet_genre| %>
      <li>
        <%= pet_genre.name %>
        <%= link_to '削除', admin_pet_genre_path(pet_genre), method: :delete, data: { confirm: '本当に削除しますか？' } %> 
      </li>
    <% end %>
  </ul>
</div>
</div>
</div>